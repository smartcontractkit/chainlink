{
  "name": "chainlink",
  "version": "0.8.1",
  "license": "MIT",
  "main": "./dist/src/generated",
  "scripts": {
    "generate-typings": "typechain --target ethers --outDir src/generated dist/artifacts/**/*",
    "postgenerate-typings": "yarn workspace @chainlink/test-helpers export-typings ../evm/src/generated ../evm/dist/src/generated",
    "build": "sol-compiler && yarn generate-typings && tsc -b",
    "build:windows": "truffle.cmd build",
    "depcheck": "echo 'chainlink' && depcheck --ignore-dirs=build/contracts,v0.5,box,v0.6 || true",
    "solhint": "solhint ./contracts/**/*.sol",
    "lint": "yarn solhint",
    "slither": "truffle compile --quiet && slither . && rimraf dist/artifacts",
    "pretest": "yarn build",
    "test": "jest --testTimeout 80000",
    "prepublishOnly": "yarn setup && yarn lint && yarn test",
    "setup": "ts-node ./scripts/build",
    "truffle:migrate:cldev": "truffle migrate --network cldev"
  },
  "dependencies": {
    "@chainlink/test-helpers": "0.0.1",
    "cbor": "^5.0.1",
    "ethers": "^4.0.41",
    "link_token": "^1.0.6",
    "openzeppelin-solidity": "^1.12.0"
  },
  "devDependencies": {
    "@0x/sol-compiler": "^4.0.3",
    "@types/cbor": "^5.0.0",
    "@types/chai": "^4.2.4",
    "@types/jest": "^24.0.25",
    "@types/node": "^12.7.5",
    "@types/shelljs": "^0.8.6",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "depcheck": "^0.9.1",
    "execa": "^3.2.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "rimraf": "^3.0.0",
    "shelljs": "^0.8.3",
    "solhint": "^2.1.0",
    "truffle": "^5.0.25",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typechain": "1.0.3",
    "typechain-target-ethers": "^1.0.1",
    "typescript": "^3.7.4"
  },
  "files": [
    "contracts",
    "v0.5/contracts",
    "v0.6/contracts",
    "dist",
    "zos.json",
    "zos.rinkeby.json",
    "zos.ropsten.json"
  ]
}
