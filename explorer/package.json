{
  "name": "@chainlink/explorer",
  "private": true,
  "version": "0.8.3",
  "description": "LINK Explorer",
  "author": "Chainlink Dev Team",
  "license": "MIT",
  "keywords": [],
  "engines": {
    "node": "~10.16"
  },
  "scripts": {
    "admin:seed": "ts-node src/bin/admin.ts seed",
    "admin:clnodes:add": "ts-node src/bin/clnodes.ts add",
    "admin:clnodes:delete": "ts-node src/bin/clnodes.ts delete",
    "depcheck": "echo \"@chainlink/explorer\" && depcheck --ignore-dirs=client || exit 0",
    "predev": "yarn automigrate",
    "dev": "concurrently \"yarn dev:server\" \"yarn dev:client\" ",
    "dev:server:docker": "TYPEORM_NAME=docker EXPLORER_COOKIE_SECRET=testingtestingtestingtestingtesting  ts-node-dev --transpileOnly ./src/index.ts",
    "dev:client": "yarn workspace @chainlink/explorer-client run start",
    "dev:server": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "dev:compose": "COMPOSE_MODE=TRUE yarn migration:run && ts-node ./src/index.ts",
    "setup": "tsc -b",
    "clean": "rimraf .migrations.*.md5",
    "prod": "yarn migration:run && ts-node ./src/index.ts",
    "write-version": "ts-node -T ./src/scripts/write-version.ts",
    "read-version": "ts-node -T ./src/scripts/read-version.ts",
    "test": "NODE_ENV=test yarn automigrate && yarn jest --runInBand",
    "test:ci": "yarn test --silent",
    "migration:run": "cross-env ts-node ./src/bin/migrator.ts migrate",
    "migration:revert": "cross-env ts-node ./src/bin/migrator.ts revert",
    "test:migration:run": "NODE_ENV=test yarn migration:run",
    "test:migration:revert": "NODE_ENV=test yarn migration:revert",
    "automigrate": "cross-env md5sum -c .migrations.${NODE_ENV:-dev}.md5 2>/dev/null || yarn migration:run && md5sum src/bin/migrator.ts src/migration/* > .migrations.${NODE_ENV:-dev}.md5"
  },
  "main": "src/index.ts",
  "dependencies": {
    "@chainlink/ts-helpers": "0.0.1",
    "argon2": "^0.25.1",
    "bignumber.js": "^9.0.0",
    "class-validator": "^0.12.1",
    "cookie-session": "^1.3.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-pino-logger": "^5.0.0",
    "helmet": "^3.21.3",
    "jayson": "^3.3.3",
    "js-sha256": "^0.9.0",
    "jsonapi-serializer": "^3.6.6",
    "local-storage-fallback": "^4.1.1",
    "mime-types": "^2.1.27",
    "pg": "^7.17.1",
    "pino": "^6.2.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.6.2",
    "typeorm": "^0.2.24",
    "ws": "^7.2.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-session": "^2.0.38",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.16.1",
    "@types/express-pino-logger": "^4.0.2",
    "@types/helmet": "0.0.47",
    "@types/jest": "^24.0.25",
    "@types/jsonapi-serializer": "^3.6.1",
    "@types/material-ui": "^0.21.6",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^13.9.1",
    "@types/node-fetch": "^2.5.5",
    "@types/pino": "^6.0.1",
    "@types/supertest": "^2.0.7",
    "@types/uuid": "^7.0.2",
    "@types/ws": "^7.2.0",
    "@types/yargs": "^15.0.5",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "depcheck": "^0.9.2",
    "http-status-codes": "^1.3.2",
    "jest": "^24.7.0",
    "node-fetch": "^2.6.0",
    "pino-pretty": "^3.6.1",
    "rimraf": "^3.0.2",
    "simple-git": "^1.132.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.7.4"
  }
}
