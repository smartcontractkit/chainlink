{
  "name": "explorer",
  "private": true,
  "version": "0.0.0",
  "description": "LINK Explorer",
  "author": "Chainlink Dev Team",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "dev": "yarn autosetup && concurrently \"cd client && yarn start\" \"ts-node-dev --respawn --transpileOnly ./src/index.ts\"",
    "dev:client": "cd client && yarn start",
    "dev:server": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "build": "yarn autoinstall && cd client && yarn build",
    "prod": "yarn migration:run && ts-node ./src/index.ts",
    "test": "cross-env TYPEORM_NAME=test yarn autosetup && yarn jest --runInBand --detectOpenHandles",
    "test-ci": "yarn autosetup && yarn jest src/__tests__ --runInBand --detectOpenHandles",
    "test-ci:e2e": "yarn autosetup && yarn build && yarn test-ci:e2e:no-build",
    "test-ci:e2e:no-build": "yarn autosetup && yarn jest e2e/ --runInBand --detectOpenHandles",
    "test-ci:silent": "yarn test-ci --silent",
    "test-ci:e2e:silent": "yarn test-ci:e2e --silent",
    "lint": "eslint --color --ext .js --ext .jsx --ext .ts --ext .tsx .",
    "lint:fix": "eslint --fix --color --ext .js --ext .jsx --ext .ts --ext .tsx .",
    "migration:run": "ts-node ./src/bin/migrator.ts migrate",
    "migration:revert": "ts-node ./src/bin/migrator.ts revert",
    "test:migration:run": "cross-env TYPEORM_NAME=test ts-node ./src/bin/migrator.ts migrate",
    "autosetup": "yarn autoinstall && yarn automigrate",
    "autoinstall": "cd client && yarn autoinstall && cd - && md5sum -c .yarn.lock.md5 || yarn install && md5sum yarn.lock > .yarn.lock.md5",
    "automigrate": "md5sum -c .migrations.${TYPEORM_NAME:-dev}.md5 2>/dev/null || yarn migration:run && md5sum src/bin/migrator.ts src/migration/* > .migrations.${TYPEORM_NAME:-dev}.md5",
    "addclnode": "ts-node src/bin/clnodes.ts add"
  },
  "main": "src/index.ts",
  "engines": {
    "node": "^10.15"
  },
  "dependencies": {
    "@types/express-winston": "^3.0.1",
    "@types/helmet": "0.0.43",
    "@types/jsonapi-serializer": "^3.6.1",
    "@types/mime-types": "^2.1.0",
    "@types/uuid": "^3.4.4",
    "express": "^4.16.4",
    "express-winston": "^3.1.0",
    "helmet": "^3.20.0",
    "js-sha256": "^0.9.0",
    "jsonapi-serializer": "^3.6.4",
    "mime-types": "^2.1.24",
    "moment": "^2.24.0",
    "pg": "^7.8.2",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.0.3",
    "typeorm": "^0.2.15",
    "typescript": "^3.5.0",
    "winston": "^3.2.1",
    "ws": "^6.2.0",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@types/expect-puppeteer": "^3.3.1",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/material-ui": "^0.21.6",
    "@types/node": "^11.11.3",
    "@types/puppeteer": "^1.12.3",
    "@types/supertest": "^2.0.7",
    "@types/whatwg-fetch": "^0.0.33",
    "@types/ws": "^6.0.1",
    "@types/yargs": "^12.0.10",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-jest": "^24.1.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "eslint": "5.12.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-prettier-standard": "^2.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-plugin-standard": "^4.0.0",
    "expect-puppeteer": "^4.1.0",
    "jest": "^24.7.0",
    "prettier": "^1.16.4",
    "puppeteer": "^1.14.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.0",
    "ts-node-dev": "^1.0.0-pre.40"
  }
}
