  "scripts": {
    "start": "yarn build:tsc && react-static start",
    "build": "yarn build:tsc && react-static build",
    "build:tsc": "tsc -b",
    "build:tsc:clean": "tsc -b --clean",
    "serve": "serve dist -p 3000",
    "pretest": "yarn build:tsc",
    "test": "jest",
    "test:ci": "yarn test --coverage --reporters jest-silent-reporter --maxWorkers=50%",
    "watch": "yarn test --watchAll --notify",
    "setup": "echo \"No setup required for @chainlink/operator-ui\"",
    "setup:test": "yarn workspace @chainlink/ts-test-helpers run setup && yarn workspace @chainlink/operator-ui run setup:dependencies",
    "setup:dependencies": "yarn workspace @chainlink/json-api-client run build && yarn workspace @chainlink/redux run build && yarn workspace @chainlink/local-storage run build && yarn workspace @chainlink/styleguide run build",
    "depcheck": "echo '@chainlink/operator-ui' && depcheck --ignore-dirs=dist || true"
  },
