{
  "name": "@chainlink/operator-ui",
  "version": "0.6.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "react-static start",
    "build": "yarn build:tsc && react-static build",
    "build:tsc": "tsc -b",
    "build:tsc:clean": "tsc -b --clean",
    "serve": "serve dist -p 3000",
    "prelint": "yarn build:tsc",
    "eslint": "eslint --ext .js,.ts,.tsx,.jsx .",
    "lint": "tsc --noEmit && yarn eslint",
    "format": "prettier --write \"**/*\"",
    "pretest": "yarn build:tsc",
    "test": "jest",
    "test:ci": "yarn test --coverage --reporters jest-silent-reporter --maxWorkers=50%",
    "watch": "yarn test --watchAll --notify",
    "depcheck": "echo '@chainlink/operator-ui' && depcheck --ignore-dirs=dist || true"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@chainlink/styleguide": "0.0.0",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.1",
    "bignumber.js": "^8.0.1",
    "change-case": "^3.0.2",
    "classnames": "^2.2.6",
    "core-js": "^3.2.1",
    "formik": "^1.0.3",
    "humps": "^2.0.1",
    "isomorphic-unfetch": "^3.0.0",
    "javascript-stringify": "^2.0.0",
    "json-api-normalizer": "^0.4.13",
    "json-pretty-html": "^1.1.2",
    "local-storage-fallback": "^4.1.1",
    "lodash": "^4.17.13",
    "moment": "^2.24.0",
    "normalize-url": "^4.3.0",
    "numeral": "^2.0.6",
    "path-to-regexp": "^3.0.0",
    "promise.prototype.finally": "^3.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.7.0",
    "react-jss": "^8.6.1",
    "react-redux": "~6.0.0",
    "react-resize-detector": "^3.4.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-universal-component": "^3.0.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-object": "^0.5.6",
    "redux-thunk": "^2.3.0",
    "url": "^0.11.0",
    "use-react-hooks": "^1.0.7",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@chainlink/eslint-config": "0.0.1",
    "@chainlink/prettier-config": "0.0.1",
    "@types/classnames": "^2.2.8",
    "@types/enzyme": "^3.10.3",
    "@types/fetch-mock": "^7.3.1",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.138",
    "@types/node": "^11.9.5",
    "@types/numeral": "^0.0.25",
    "@types/path-to-regexp": "^1.7.0",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.7.0",
    "@types/react-redux": "~6.0.0",
    "@types/react-resize-detector": "^4.0.1",
    "@types/react-router": "^5.0.3",
