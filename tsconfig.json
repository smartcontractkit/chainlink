{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "lib": ["es2017"],
    "incremental": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "isolatedModules": false,
    "noEmit": true,
    "allowJs": true,
    "jsx": "preserve",
    "sourceMap": true,
    "removeComments": false,
    "strict": true,
    "esModuleInterop": true
  }
}
